{
  "name": "ras-ui-storybook",
  "version": 2,
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "buildCommand": "pnpm build-storybook",
  "outputDirectory": "packages/docs/storybook-static",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": null,
  "ignoreCommand": "git diff --quiet HEAD^ HEAD packages/docs/ packages/react/src/ packages/tokens/",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}